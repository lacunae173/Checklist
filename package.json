    "react-scripts": "4.0.3",
    "react-scroll": "^1.8.4",
    "web-vitals": "^1.0.1"
  },
  "main": "public/main.js",
  "homepage": "./",
  "scripts": {
    "start": "electron-forge start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package": "electron-forge package",
    "make": "react-scripts build && electron-forge make"
